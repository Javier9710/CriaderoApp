<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProveedorActivity">

    <FrameLayout

        android:id="@+id/Proveedor"
        android:layout_width="404dp"
        android:layout_height="43dp"
        android:gravity="center_vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="20dp">


        <TextView

            android:id="@+id/id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="0dp"
            android:text="ID  |"></TextView>

        <TextView
            android:id="@+id/nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="50dp"
            android:text="CODIGO   |"></TextView>

        <TextView
            android:id="@+id/cantidad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginRight="50dp"
            android:text="NOMBRE   |"></TextView>

        <TextView
            android:id="@+id/proveedor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="190dp"
            android:layout_marginRight="10dp"
            android:text="DIRECCION    |"></TextView>

        <TextView
            android:id="@+id/responsable"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="290dp"
            android:layout_marginRight="0dp"
            android:text="  RESPONSABLE"></TextView>


    </FrameLayout>

    <Button
        android:id="@+id/button2"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/plus"
        android:text=""
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.939"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>